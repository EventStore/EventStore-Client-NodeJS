{
  "name": "eventstore-client-nodejs",
  "version": "1.0.0",
  "description": "Client API for Event Store v6 for NodeJS",
  "main": "./dist/index.js",
  "scripts": {
    "build": "run-s generate build:*",
    "build:ts": "tsc",
    "lint": "run-s -c lint:*",
    "lint:prettier": "prettier --check \"src/**/**/!(*.d).{ts,json}\"",
    "lint:eslint": "eslint \"src/**/*.ts\"",
    "test": "jest",
    "generate": "./scripts/build-protos.sh",
    "clean": "rm -rf ./dist"
  },
  "files": [
    "dist/",
    "generated/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EventStore/EventStore-Client-NodeJS.git"
  },
  "keywords": [
    "EventStore",
    "EventSourcing"
  ],
  "author": "Event Store Limited",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/EventStore/EventStore-Client-NodeJS/issues"
  },
  "homepage": "https://github.com/EventStore/EventStore-Client-NodeJS#readme",
  "jest": {
    "preset": "ts-jest"
  },
  "dependencies": {
    "@grpc/proto-loader": "^0.5.5",
    "@types/google-protobuf": "^3.7.2",
    "google-protobuf": "^4.0.0-rc.2",
    "grpc": "^1.24.3",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.5",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "eslint": "^7.5.0",
    "grpc-tools": "^1.9.1",
    "grpc_tools_node_protoc_ts": "^4.1.0",
    "jest": "^26.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.3",
    "typescript": "^3.9.7"
  }
}
