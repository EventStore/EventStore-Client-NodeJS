{
  "name": "test",
  "version": "0.0.1",
  "private": true,
  "description": "KurrentDB Client Tests",
  "scripts": {
    "build": "pnpm run build:ts",
    "build:ts": "tsc --build",
    "build:watch": "pnpm run build:ts-watch",
    "build:ts-watch": "tsc --build --watch",
    "clean": "pnpm run \"/^clean:.*/\"",
    "clean:dist": "shx rm -rf ./dist",
    "clean:buildinfo": "shx rm -rf *.tsbuildinfo",
    "clean:test-instances": "shx rm -rf ./src/utils/instances",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EventStore/EventStore-Client-NodeJS.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/EventStore/EventStore-Client-NodeJS/issues"
  },
  "homepage": "https://developers.kurrent.io/clients/grpc/getting-started?codeLanguage=NodeJS",
  "keywords": [
    "EventStore",
    "EventSourcing",
    "Test"
  ],
  "dependencies": {
    "@kurrent/db-client": "workspace:*",
    "@kurrent/opentelemetry": "workspace:*"
  },
  "devDependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@grpc/grpc-js": "^1.12.4",
    "@opentelemetry/instrumentation": "^0.56.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.51.1",
    "@opentelemetry/sdk-trace-node": "^1.30.0",
    "@opentelemetry/semantic-conventions": "^1.28.0",
    "@types/jest": "^29.5.14",
    "docker-compose": "^0.24.8",
    "dotenv": "^16.4.7",
    "get-port": "^5.1.1",
    "shx": "^0.3.4",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.5",
    "@types/uuid": "catalog:",
    "@types/debug": "catalog:",
    "debug": "catalog:",
    "uuid": "catalog:"
  }
}